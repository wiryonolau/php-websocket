{
    "name": "itseasy/php-websocket",
    "description": "PHP Websocket Server",
    "type": "library",
    "keywords": [
        "amphp",
        "websocket"
    ],
    "config": {
        "platform": {
            "php": "7.4.27",
            "ext-sockets": "1"
        }
    },
    "require": {
        "php": "^7.4",
        "amphp/websocket-server": "^2.0",
        "amphp/http-server": "^2.1",
        "amphp/http-server-router": "^1.0",
        "symfony/console": "^5.3",
        "php-di/php-di": "^6.3",
        "laminas/laminas-stdlib": "^3.4",
        "laminas/laminas-log": "^2.13",
        "amphp/websocket-client": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Itseasy\\Websocket\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itseasy\\Websocket\\Test\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [{
        "name": "Wiryonolau",
        "email": "wiryonolau@gmail.com"
    }],
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "laminas/laminas-config-aggregator": "^1.4",
        "amphp/phpunit-util": "^2.0"
    }
}
